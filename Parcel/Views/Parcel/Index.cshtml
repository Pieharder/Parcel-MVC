@using Parcels.Models;

<h1>The Not Shitty Parcel Delivery Service</h1>

@if (Model.Count == 0)
{
  <p>You must enter parcel details!</p>
}
@foreach (Item item in Model)
{
  <h3>@item.Title</h3>
  <ul>
    <li>@item.Description</li>
    <li>@item.Contact</li>
  </ul>
}
<a href="/items/new">Send your package!</a>